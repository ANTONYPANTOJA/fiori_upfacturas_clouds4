{
    "version": "0.2.0",
    "configurations": [
      
      {
        "name": "runappupload",
        "cwd": "${workspaceFolder}",
        "runtimeArgs": [
          "fiori",
          "run"
        ],
        "type": "node",
        "request": "launch",
        "runtimeExecutable": "npx",
        "args": [
          "--open",
          "index.html"
        ],
        "windows": {
          "runtimeExecutable": "npx.cmd"
        },
        "console": "internalConsole",
        "internalConsoleOptions": "openOnSessionStart",
        "outputCapture": "std",
        "env": {
          "run.config": "{\"handlerId\":\"fiori_tools\",\"runnableId\":\"/home/user/projects/Z_APP_UPLOADINVOICES\"}",
          "FIORI_TOOLS_BACKEND_CONFIG": "[{\"destination\":\"DATEC_S4_CLOUD_DEV\",\"path\":\"/sap\"}]",
          "FIORI_TOOLS_URL_PARAMS": "sap-ui-xx-viewCache=false"
        }
      }
    ]
  }